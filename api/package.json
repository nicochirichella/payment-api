{
  "name": "payment-api",
  "version": "1.0.0",
  "description": "Trocafone Payment API",
  "main": "dist/main.js",
  "scripts": {
    "only-test": "NODE_ENV=test ./node_modules/.bin/babel-node node_modules/.bin/_mocha --require babel-core/register --sort --timeout 15000 --recursive -R spec test/**/*",
    "old-test": "NODE_ENV=test ./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --include-all-sources true --root ./src -- --require babel-core/register --sort --timeout 15000 --recursive -R spec test/**/*",
    "test": "NODE_ENV=test node_modules/.bin/nyc mocha test/**/*",
    "start": "node dist/start.js",
    "start-dev": "nodemon start dist/start.js --watch dist",
    "dev": "npm run build && ./node_modules/.bin/concurrently --kill-others \"npm run build-watch\" \"npm run start-dev\"",
    "build": "./node_modules/.bin/babel src -d dist --copy-files",
    "build-watch": "./node_modules/.bin/babel src -d dist --watch --copy-files --source-maps",
    "lint": "./node_modules/.bin/eslint src"
  },
  "author": "Trocafone",
  "license": "ISC",
  "dependencies": {
    "@cybersource/flex-sdk-node": "^0.1.0",
    "@trocafone/datadog-client": "^1.0.2",
    "@trocafone/logger": "^1.0.4",
    "bluebird": "^2.9.34",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.9.1",
    "bookshelf-paranoia": "^0.13.1",
    "bunyan": "^1.5.1",
    "bunyan-format": "^0.2.1",
    "checkit": "^0.7.0",
    "convict": "^1.0.1",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "cuid": "^1.3.8",
    "datadog-metrics": "^0.8.1",
    "express": "^4.16.4",
    "express-validator": "^2.13.0",
    "express-xml-bodyparser": "^0.2.2",
    "handlebars": "^4.0.12",
    "json5": "^2.1.0",
    "knex": "^0.8.6",
    "lodash": "^3.10.0",
    "moment": "^2.23.0",
    "moment-business-time": "^0.7.1",
    "moment-timezone": "^0.4.0",
    "morgan": "^1.9.1",
    "newrelic": "^4.12.0",
    "nodemon": "^2.0.2",
    "nyc": "^13.1.0",
    "pg": "^4.4.0",
    "promise-retry": "^1.1.1",
    "request": "^2.88.0",
    "require-tree": "^1.1.1",
    "soap": "^0.25.0",
    "tv4": "^1.2.7",
    "uuid": "^2.0.1",
    "xml2js": "^0.4.15"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.3.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "chai-xml": "^0.3.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^2.2.5",
    "mockery": "^1.7.0",
    "nock": "^8.0.0",
    "sinon": "^1.17.2",
    "sqlite3": "^3.1.1",
    "supertest": "^1.0.1"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  }
}
